<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_me">

    <fragment
        android:id="@+id/navigation_me"
        android:name="com.example.familyapp.ui.me.HomeFragment"
        android:label="Me"
        tools:layout="@layout/fragment_me" />

    <fragment
        android:id="@+id/navigation_family"
        android:name="com.example.familyapp.ui.myfamily.DashboardFragment"
        android:label="Family"
        tools:layout="@layout/fragment_myfamily" />

    <fragment
        android:id="@+id/navigation_my_friends"
        android:name="com.example.familyapp.ui.myfriends.MyFriendsFragment"
        android:label="My friends"
        tools:layout="@layout/fragment_myfriends" />
</navigation>